{
  "name": "object-path-plus",
  "version": "1.3.0",
  "description": "Object path traversal with concatenation",
  "main": "dist/index.js",
  "repository": "https://github.com/mixmaxhq/object-path-plus",
  "author": "Trey Tacon <trey@mixmax.com> (https://mixmax.com)",
  "license": "MIT",
  "dependencies": {
    "object-path": "^0.11.4"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "ava-spec": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-mixmax": "^0.6.0",
    "nyc": "^11.2.1"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && babel src -d dist",
    "watch": "babel --watch src/ -d dist",
    "lint": "eslint .",
    "report": "nyc ava",
    "test": "ava",
    "prepublishOnly": "npm run build",
    "ci": "npm run lint && npm run build && npm run test"
  },
  "ava": {
    "timeout": "60s",
    "files": [
      "spec/**/*Spec.js"
    ]
  },
  "nyc": {
    "exclude": [
      "index.js",
      "spec"
    ]
  }
}